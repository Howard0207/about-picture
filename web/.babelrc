{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "entry",
                "corejs": {
                    "version": 3, // 使用core-js@3
                    "proposals": true
                }
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }],
        ["@babel/plugin-proposal-class-properties", { "loose": true }]
    ],
    "compact": false
}
